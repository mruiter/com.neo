{
  "name": "homey-zwavedriver",
  "version": "2.1.10",
  "description": "This module can be used to make the development of Z-Wave apps for Homey easier.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive",
    "lint": "eslint .",
    "serve": "concurrently \"serve build/\" \"npm run build:watch\"",
    "build": "jsdoc --configure ./docs/jsdoc.json",
    "build:clean": "rm -rf ./build",
    "build:watch": "watch \"npm run build:clean && npm run build\" lib docs \"node_modules/homey-jsdoc-template\""
  },
  "keywords": [
    "zwave",
    "homey"
  ],
  "author": "Athom B.V.",
  "license": "ISC",
  "dependencies": {
    "color-space": "^1.16.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-athom": "^2.0.6",
    "homey": "^2.1.5",
    "homey-jsdoc-template": "github:athombv/homey-jsdoc-template#1.5",
    "jsdoc": "^3.6.6",
    "jsdoc-ts-utils": "^1.1.2",
    "mocha": "^7.1.1",
    "npm-watch": "^0.6.0",
    "serve": "^11.3.1",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/athombv/node-homey-zwavedriver.git"
  },
  "bugs": {
    "url": "https://github.com/athombv/node-homey-zwavedriver/issues"
  },
  "homepage": "https://github.com/athombv/node-homey-zwavedriver#readme",
  "watch": {
    "jsdoc": {
      "patterns": [
        "lib"
      ],
      "extensions": [
        "js"
      ]
    }
  }
}
